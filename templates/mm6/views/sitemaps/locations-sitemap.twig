<?xml version="1.0" encoding="UTF-8"?><?xml-stylesheet type="text/xsl" href="{{- site.baseurl -}}/sitemap/styles-sitemap.xsl"?>
<urlset xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:image="http://www.google.com/schemas/sitemap-image/1.1"
        xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9 http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd http://www.google.com/schemas/sitemap-image/1.1 http://www.google.com/schemas/sitemap-image/1.1/sitemap-image.xsd"
        xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {% if data.houses|length > 1 %}
    {% for house in data.houses %}
        <url>
            <loc>{{- site.baseurl -}}{{- house.url -}}</loc>
            <changefreq>hourly</changefreq>
            <priority>0.5</priority>
            {% if house.photo is defined and house.photo != '' %}
                <image:image>
                    {% if house.photo starts with '//' %}
                        {% set image = 'http:' ~ house.photo %}
                    {% else %}
                        {% set image = house.photo %}
                    {% endif %}
                    <image:loc>{{- image -}}</image:loc>
                    <image:title><![CDATA[{{ house.name }}]]></image:title>
                    <image:caption><![CDATA[Image of {{ house.name }}]]></image:caption>
                </image:image>
            {% endif %}
        </url>
    {% endfor %}
    {% endif %}
</urlset>
