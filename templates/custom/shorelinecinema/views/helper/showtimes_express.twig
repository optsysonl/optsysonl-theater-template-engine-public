{%- for showdate, houses in showtimes -%}
    {% set comments = houses.houses|first %}
    {%- for comment, times in comments|sort -%}
        {%- for time, time_item in times|first -%}
            <span title="$st.label">
                <a href="/ticket/{{ time_item.details.screens_id }}">  {{- time|date('g:ia') -}}</a>
            </span>
        {%- endfor -%}
    {%- endfor -%}
{%- endfor -%}