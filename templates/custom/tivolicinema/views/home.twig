{% set house_id = data.houses|first.house_id %}
{% extends "common/base.twig" %}

{% block meta_title %}{{ parent() }} | Home {% endblock %}

{% block content %}
    {{ include('common/header.twig') }}

    <div id="intro">
        {{ include('helper/featured_home.twig', {movies: data.movies, showdates:data.showdates}, with_context = false) }}
    </div>

    <div id="lower">
        <div class="col3 first">
            {{ data.pages.10343.content|raw }}
        </div>
        <div class="col3">
            <h3>Visit Us</h3>
            <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3148.6982499859137!2d175.46851580000003!3d-37.89073889999997!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x6d6d0282b2b5fb4f%3A0x4c920bfcc22859de!2s32+Lake+St!5e0!3m2!1sen!2snz!4v1395099990450" width="100%" height="250" frameborder="0" style="border:0"></iframe>
        </div>
        <div class="col3">
            {{ include('helper/signup.twig', {cust_id:site.config['cust_id'], house_id:data.houses|first.house_id}, with_context = false) }}
        </div>
    </div>

    {{ include('common/footer.twig') }}
{% endblock %}

