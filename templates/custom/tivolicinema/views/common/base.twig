<!DOCTYPE html>
<!--[if lt IE 7]> <html class="no-js lt-ie9 lt-ie8 lt-ie7" lang="en"> <![endif]-->
<!--[if IE 7]>    <html class="no-js lt-ie9 lt-ie8" lang="en"> <![endif]-->
<!--[if IE 8]>    <html class="no-js lt-ie9" lang="en"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js" lang="en"> <!--<![endif]-->

<head>
    {% block head %}
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <title>{% block meta_title %}{{ site.config.title }}{% endblock %}</title>
        <meta name="description" content="{% block meta_description %}{{- site.config.description -}}{% endblock %}">

        <meta name="author" content="Webedia Movies Pro, info@webedia.us">
        <meta name="viewport" content="width=device-width">
        <meta property="og:site_name" content="{{ site.config.title }} Website"/>


        <script src="/{$id}/js/libs/modernizr-2.5.3.min.js"></script>
        <!-- jQuery -->
        <script src="https://code.jquery.com/jquery-3.2.1.min.js" integrity="sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4=" crossorigin="anonymous"></script>
        <!-- jQuery UI -->
        <script type="text/javascript" src="//ajax.googleapis.com/ajax/libs/jqueryui/1.10.4/jquery-ui.min.js"></script>

        <!-- add Favicon -->
        {% if site.meta['icon'] is defined %}<link rel="icon" href="{{ site.meta['icon'] }}" sizes="16x16 32x32 96x96" type="image/x-icon">{% endif %}

        {% block css %}
        <!-- CSS -->
        {% block meta_google_fonts %}{{ site.meta['google_fonts']|raw }}{% endblock %}
    <link rel="stylesheet" type="text/css" href="/assets/css/editor.css">
    <link rel="stylesheet" type="text/css" href="/assets/css/style.css">
        <!-- Fancybox -->
    <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/fancybox/2.1.6/css/jquery.fancybox.min.css">

        {% endblock %}

        {% block js_scripts %}
        <!-- JS -->
        <!-- jwPlayer -->
        <script type="text/javascript" src="/assets/files/jwplayer/jwplayer.js"></script>
        <!-- Fancybox -->
        <script src="https://cdnjs.cloudflare.com/ajax/libs/fancybox/2.1.6/js/jquery.fancybox.min.js"></script>

        {% endblock %}

    {% block js_analytics %}
        {% if site.meta['analytics'] is not empty %}
        <!-- Google Analytics -->
        <script>
            // JavaScript Document
            (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
                m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
            })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

            ga('create', '{{ site.meta['analytics'] }}', '');
            ga('send', 'pageview');
        </script>
        {% endif %}
    {% endblock %}

    {% endblock %}
</head>
<body id="{{- this_page.class['body'] -}}">
{% block content %}{% endblock %}

{% block js_scripts_footer %}

{% endblock %}
</body>
</html>