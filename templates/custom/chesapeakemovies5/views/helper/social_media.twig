<div class="social-icons">
    {%- if icons.facebook.link is defined and icons.facebook.link != '' -%}
        <a href='{{ icons.facebook.link }}' target='_blank'><img alt="Facebook icon" src='{{ icons.facebook.img }}' class='svg social-icon'></a>
    {%- endif %}
    {% if icons.twitter.link is defined and icons.twitter.link != '' -%}
        <a href='{{ icons.twitter.link }}' target='_blank'><img alt="Twitter icon" src='{{ icons.twitter.img }}' class='svg social-icon'></a>
    {%- endif %}
    {% if icons.instagram.link is defined and icons.instagram.link != '' -%}
        <a href='{{ icons.instagram.link }}' target='_blank'><img alt="Instagram icon" src='{{ icons.instagram.img }}' class='svg social-icon'></a>
    {%- endif %}
    {% if icons.pintrest.link is defined and icons.pintrest.link != '' -%}
        <a href='{{ icons.pintrest.link }}' target='_blank'><img alt="Pinterest icon" src='{{ icons.pintrest.img }}' class='svg social-icon'></a>
    {%- endif %}
    {% if icons.google_plus.link is defined and icons.google_plus.link != '' -%}
        <a href='{{ icons.google_plus.link }}' target='_blank'><img alt="Google Plus icon" src='{{ icons.google_plus.img }}' class='svg social-icon'></a>
    {%- endif %}
    {% if icons.site_google_plus.link is defined and icons.site_google_plus.link != '' -%}
        <a href='{{ icons.site_google_plus.link }}' target='_blank'><img alt="Google Plus icon" src='{{ icons.site_google_plus.img }}' class='svg social-icon'></a>
    {%- endif %}
    {% if config.template_options.nl_signup_url is defined and config.template_options.nl_signup_url is not empty %}
        <a href='{{- config.template_options.nl_signup_url -}}' data-fancybox-type='iframe' class='newsletter-svg' target='_blank'><img alt="Newsletter sign up icon" src='/assets/img/email.svg' class='svg social-icon'></a>
    {% elseif icons.cust_id.link is defined and icons.cust_id.link != '' %}
        <a href='https://csxutils.fbmovies.cinema-source.com/subscribe/?cust_id={{- icons.cust_id.link -}}&amp;iframe=true&amp;width=600&amp;height=400' data-fancybox-type='iframe' class='newsletter-svg' target='_blank'><img alt="Newsletter sign up icon" src='{{ icons.cust_id.img }}' class='svg social-icon'></a>
    {% endif %}
</div>