{%- if icons.facebook.link is defined and icons.facebook.link != '' -%}
    <a href='{{ icons.facebook.link }}' class='icon-wrapper' style='text-decoration:none;' target='_blank' title='Facebook Link'>
        <font style='display:none'>Facebook Link</font>
        <img src='{{ icons.facebook.img }}' class='svg'>
    </a>
{%- endif %}
